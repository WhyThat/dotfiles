{
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.hoverTarget": "preview",
  "coc.preferences.echodocSupport": true,
  "languageserver": {
    "ocaml": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocaml-language-server", "--stdio"],
      "filetypes": ["ocaml", "reason", "reasonjest",  "reasonstories", "reasonreact"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "snippets": {
    "extends": {
      "reasonjest": ["reason"],
      "reasonstories": ["reason"],
      "reasonreact": ["reason"]
    }
  },
  "tsserver.npm": "/home/mathieu/.fnm/node-versions/v10.16.3/installation/bin/npm",
  "eslint.options": {"configFile": ".eslintrc.json"}
}
